{
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "rewrite": "/api/index"
        },
        {
            "route": "/*",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}