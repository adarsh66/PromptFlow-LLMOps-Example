{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{png,jpg,gif,svg}",
            "*.css"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    }
}